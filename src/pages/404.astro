---
import Layout from "../layouts/Layout.astro";

const title = "Not found";

export const prerender = false;

---

<Layout title={title}>
	<div class="row">
		<div class="col-md-8">
            <h1>{title}</h1>

            <p><strong>Error 404</strong>: The page you requested could not be found. </p>
		</div>
	</div>
</Layout>